<MvFUNCTION NAME = "OrderSkinsComponentModule_Description" PARAMETERS = "module var, item var" STANDARDOUTPUTLEVEL = "">
	<MvASSIGN NAME = "l.pages" VALUE = "{ [ g.Module_Feature_TUI_DB ].Item_Load_PageList( l.item ) }">

	<MvFUNCTIONRETURN VALUE = "{ 'Exports Invoice Order Contents template from pages: ' $ l.pages }">
</MvFUNCTION>

<MvIFNDEF NAME = "ORDER_COMPONENT_NOAPIFUNCTIONS">
<MvFUNCTION NAME = "SkinsComponentModule_Export_Item" PARAMETERS = "module var, item, output var" STANDARDOUTPUTLEVEL = "">
	<MvFUNCTIONRETURN VALUE = "{ OrderSkinsComponentModule_Export_Item( l.module, l.item, l.output ) }">
</MvFUNCTION>
</MvIFNDEF>

<MvFUNCTION NAME = "OrderSkinsComponentModule_Export_Item" PARAMETERS = "module var, item, output var" STANDARDOUTPUTLEVEL = "">
	<MvCAPTURE VARIABLE = "l.output" STANDARDOUTPUTLEVEL = "text, html, compresswhitespace">
		<MvFOREACH ITERATOR = "l.page" ARRAY = "l.pages" COUNT = "{ [ g.Module_Feature_TUI_DB ].PageList_Load_Item_Runtime( l.item:id, l.pages ) }">
			<MvREFERENCEARRAY NAME = "l.settings" VARIABLE = "l.page:settings">
				<MvMEMBER NAME = "{ l.item:code }">
			</MvREFERENCEARRAY>

			<Page_Update code="{ l.page:code }">
				<Item code="{ l.item:code }">
					<Fields>
						<MvFOREACH ITERATOR = "l.customfield" ARRAY = "l.settings:fields_custom">
							<Custom module="{ l.customfield:module }" code="{ l.customfield:code }" />
						</MvFOREACH>

						<MvFOREACH ITERATOR = "l.customfield" ARRAY = "l.settings:fields_custom_order">
							<CustomOrder module="{ l.customfield:module }" code="{ l.customfield:code }" />
						</MvFOREACH>
					</Fields>

					<ImageTypes>
						<MvFOREACH ITERATOR = "l.imagetype" ARRAY = "l.settings:imagetypes">
							<ImageType><MvEVAL EXPR = "{ miva_cdata_encode( l.imagetype ) }"></ImageType>
						</MvFOREACH>
					</ImageTypes>

					<MvIF EXPR = "{ l.settings:constrain NE 'B' }">
						<ImageDimensions constrain="No" />
					<MvELSE>
						<ImageDimensions constrain="Yes">
							<Width><MvEVAL EXPR = "{ int( l.settings:b_width ) }"></Width>
							<Height><MvEVAL EXPR = "{ int( l.settings:b_height ) }"></Height>
						</ImageDimensions>
					</MvIF>

					<MvIF EXPR = "{ l.settings:groups }">
						<LineItemDiscounts>Grouped</LineItemDiscounts>
					<MvELSE>
						<LineItemDiscounts>Separate</LineItemDiscounts>
					</MvIF>

					<DisplayLineItemDiscountDetails><MvIF EXPR = "{ l.settings:lineitemdiscounts }">Yes<MvELSE>No</MvIF></DisplayLineItemDiscountDetails>

					<MvIF EXPR = "{ l.settings:lineitemprice EQ 'retail' }">
						<AdditionalPriceDisplay>Retail</AdditionalPriceDisplay>
					<MvELSEIF EXPR = "{ l.settings:lineitemprice EQ 'base' }">
						<AdditionalPriceDisplay>Base</AdditionalPriceDisplay>
					<MvELSE>
						<AdditionalPriceDisplay>None</AdditionalPriceDisplay>
					</MvIF>

					<DisplayCouponLineItems><MvIF EXPR = "{ l.settings:lineitemcoupons }">Yes<MvELSE>No</MvIF></DisplayCouponLineItems>

					<MvIFDEF NAME = "ORDER_COMPONENT_PAYMENTS">
						<DisplayPayments><MvIF EXPR = "{ l.settings:payments }">Yes<MvELSE>No</MvIF></DisplayPayments>
					</MvIFDEF>

					<MvIF EXPR = "{ l.settings:advanced }">
						<MvIF EXPR = "{ [ g.Module_Feature_TUI_DB ].ManagedTemplateVersion_Load_Filename_Current( l.settings:template_filename, l.templateversion ) }">
							<Template><MvEVAL EXPR = "{ miva_cdata_encode( l.templateversion:source ) }"></Template>
						</MvIF>
					</MvIF>

					<Notes>#Set_Current_Time#</Notes>
				</Item>
			</Page_Update>
		</MvFOREACH>
	</MvCAPTURE>
</MvFUNCTION>

<MvFUNCTION NAME = "OrderComponent_Initialize_Payments" PARAMETERS = "order_id, destination var" STANDARDOUTPUTLEVEL = "">
	<MvFOREACH ITERATOR = "l.orderpayment" ARRAY = "l.orderpayments" COUNT = "{ [ g.Module_Library_DB ].OrderPaymentList_Load_Order( l.order_id, l.orderpayments ) }">
		<MvIF EXPR = "{ l.orderpayment:pay_id LE 0 }">
			<MvFOREACHCONTINUE>
		<MvELSEIF EXPR = "{ ( l.orderpayment:type EQ 2 ) OR
							( l.orderpayment:type EQ 4 ) OR
							( l.orderpayment:type EQ 7 ) }">
			<MvCOMMENT>
			|
			| Ignore capture only and void transactions
			|
			</MvCOMMENT>

			<MvFOREACHCONTINUE>
		</MvIF>

		<MvREFERENCEARRAY NAME = "l.payment_module" VARIABLE = "g.Session:cache:cmp_include_order:modules">
			<MvDIMENSION INDEX = "{ l.orderpayment:pay_id }">
		</MvREFERENCEARRAY>

		<MvIF EXPR = "{ l.payment_module:loaded EQ 0 }">
			<MvIF EXPR = "{ [ g.Module_Library_DB ].Module_Load_ID( l.orderpayment:pay_id, l.payment_module ) }">
				<MvASSIGN NAME = "l.payment_module:loaded"		VALUE = 1>
			<MvELSE>
				<MvASSIGN NAME = "l.payment_module:loaded"		VALUE = -1>
			</MvIF>
		</MvIF>

		<MvIF EXPR = "{ l.payment_module:loaded EQ -1 }">
			<MvFOREACHCONTINUE>
		</MvIF>

		<MvASSIGN NAME = "l.payment:desc"						VALUE = "{ [ g.Module_Feature_TUI_UT ].CommonComponentFields_PaymentMethod_Description( l.payment_module, l.orderpayment:pay_data ) }">

		<MvASSIGN NAME = "l.payment:type"						VALUE = "{ l.orderpayment:type }">

		<MvASSIGN NAME = "l.payment:amount"						VALUE = "{ l.orderpayment:amount }">
		<MvASSIGN NAME = "l.payment:amount_negative"			VALUE = "{ 0 - l.orderpayment:amount }">

		<MvASSIGN NAME = "l.payment:formatted_amount"			VALUE = "{ [ g.Module_Root $ g.Store:currncy_mod:module ].CurrencyModule_AddFormatting( g.Store:currncy_mod, l.payment:amount ) }">
		<MvASSIGN NAME = "l.payment:formatted_amount_negative"	VALUE = "{ [ g.Module_Root $ g.Store:currncy_mod:module ].CurrencyModule_AddFormatting( g.Store:currncy_mod, l.payment:amount_negative ) }">

		<MvASSIGN NAME = "l.null"								VALUE = "{ miva_array_insert( l.destination, l.payment, -1 ) }">
	</MvFOREACH>
</MvFUNCTION>
