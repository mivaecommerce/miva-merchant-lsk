<MvIFNDEF NAME = "ORDER_COMPONENT_NOAPIFUNCTIONS">
<MvFUNCTION NAME = "ComponentModule_Initialize" PARAMETERS = "module var, item, all_settings var, settings var" STANDARDOUTPUTLEVEL = "">
	<MvFUNCTIONRETURN VALUE = "{ OrderComponentModule_Initialize( l.module, l.item, l.all_settings, l.settings ) }">
</MvFUNCTION>
