<MvIFNDEF NAME = "ORDER_COMPONENT_NOAPIFUNCTIONS">
<MvFUNCTION NAME = "SkinsComponentModule_Description" PARAMETERS = "module var, item var" STANDARDOUTPUTLEVEL = "">
	<MvFUNCTIONRETURN VALUE = "{ OrderSkinsComponentModule_Description( l.module, l.item ) }">
