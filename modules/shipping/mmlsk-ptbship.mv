	<MvELSEIF EXPR = "{ g.Module_Function EQ 'PriceTableBasedShippingMethod_Delete' }">			<MvFUNCTIONRETURN VALUE = "{ JSON_PriceTableBasedShippingMethod_Delete( l.module ) }">
	<MvELSEIF EXPR = "{ g.Module_Function EQ 'PriceTableBasedShippingBreak_Insert' }">			<MvFUNCTIONRETURN VALUE = "{ JSON_PriceTableBasedShippingBreak_Insert( l.module ) }">
