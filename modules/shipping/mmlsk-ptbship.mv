		<MvIF EXPR = "{ NOT PTBSProduct_Update( l.ptbs_product ) }">
			<MvFUNCTIONRETURN VALUE = "{ [ g.Module_JSON ].JSON_Response_Error( g.Error_Code, g.Error_Message ) }">
		</MvIF>

		<MvEVAL EXPR = "{ [ g.Module_Admin ].Admin_Log_Action( 'MER-SHP-PTB-00081', 'PTBS Product \'' $ l.product:code $ '\', Method \'' $ l.method:code $ '\' updated' ) }">
	<MvELSE>
		<MvASSIGN NAME = "l.ptbs_product"				VALUE = "">
		<MvASSIGN NAME = "l.ptbs_product:method_id" 	VALUE = "{ l.method:id }">
		<MvASSIGN NAME = "l.ptbs_product:product_id"	VALUE = "{ l.product:id }">
		<MvASSIGN NAME = "l.ptbs_product:rate" 			VALUE = "{ l.rate }">

		<MvIF EXPR = "{ NOT PTBSProduct_Insert( l.ptbs_product ) }">
			<MvFUNCTIONRETURN VALUE = "{ [ g.Module_JSON ].JSON_Response_Error( g.Error_Code, g.Error_Message ) }">
		</MvIF>

		<MvEVAL EXPR = "{ [ g.Module_Admin ].Admin_Log_Action( 'MER-SHP-PTB-00082', 'PTBS Product \'' $ l.product:code $ '\', Method \'' $ l.method:code $ '\' created' ) }">
	</MvIF>

	<MvFUNCTIONRETURN VALUE = "{ [ g.Module_JSON ].JSON_Response_Success() }">
</MvFUNCTION>

