</MvFUNCTION>

<MvFUNCTION NAME = "Module_Shipping_Update" PARAMETERS = "module var" STANDARDOUTPUTLEVEL = "">
	<MvIF EXPR = "{ NOT PTBSOptions_Update( g.PTBS_Options ) }">
		<MvFUNCTIONRETURN VALUE = 0>
