<MvFUNCTION NAME = "Runtime_ProductList_Build_Query_JOIN_Attributes" PARAMETERS = "query var" STANDARDOUTPUTLEVEL = "">
	<MvEVAL EXPR = "{ SQL_Query_Data_Reference( l.query, 'Runtime_ProductList', l.query_params ) }">

