	<MvASSIGN NAME = "g.Store_Template_Path"	VALUE = "{ g.Module_Root $ 'templates/s' $ padl( l.store:id , 2, '0' ) $ '/' }">

	<MvASSIGN NAME = "l.store_directory"		VALUE = "{ 'Merchant5/s' $ padl( l.store:id , 2, '0' ) }">
	<MvASSIGN NAME = "l.null"					VALUE = "{ fmkdir( l.store_directory ) }">

	<MvASSIGN NAME = "g.Store:id"				VALUE = "{ l.store:id }">

	<MvASSIGN NAME = "l.graphicsbase"			VALUE = "{ g.imageroot $ 'graphics/' }">
	<MvASSIGN NAME = "l.null"					VALUE = "{ smkdir( l.graphicsbase ) }">
	<MvASSIGN NAME = "l.null"					VALUE = "{ schmod( l.graphicsbase, '0755' ) }">

	<MvASSIGN NAME = "l.graphicsdir"			VALUE = "{ l.graphicsbase $ padl( l.store:id, 8, '0' ) $ '/' }">
	<MvASSIGN NAME = "l.null"					VALUE = "{ smkdir( l.graphicsdir ) }">
	<MvASSIGN NAME = "l.null"					VALUE = "{ schmod( l.graphicsdir, '0755' ) }">

	<MvASSIGN NAME = "l.cssbase"				VALUE = "{ g.imageroot $ 'css/' }">
